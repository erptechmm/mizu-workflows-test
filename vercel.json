{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  ""rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
    {
      "src": "server/index.ts",
      "use": "@vercel/node",
      "config": {
        "buildCommand": "npm run build:server"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/public/index.html"
    }
  ]
}
